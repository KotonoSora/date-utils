{
	"name": "@kotonosora-tech/date-utils",
	"version": "1.0.12-alpha.2",
	"description": "A small tool calculator calendar in month",
	"main": "dist/index.js",
	"scripts": {
		"types": "tsc src/index.ts --declaration --emitDeclarationOnly --esModuleInterop --outDir dist",
		"build": "npm run build.clean && npm run build.only && npm run types",
		"build.only": "rollup -c",
		"build.watch": "rollup -c -w",
		"build.clean": "rimraf dist",
		"test": "jest",
		"test.log": "jest --coverage",
		"test.showlog": "open ./coverage/lcov-report/index.html",
		"eslint": "eslint src/**",
		"eslint.fix": "eslint src/** --fix"
	},
	"author": "kotonosora <duythang.2912@kotonosora.com>",
	"license": "MIT",
	"dependencies": {
		"date-fns": "^2.28.0"
	},
	"devDependencies": {
		"@types/jest": "^28.1.3",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"eslint": "^8.18.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-plugin-import": "^2.26.0",
		"jest": "^28.1.2",
		"rimraf": "^3.0.2",
		"rollup": "^2.75.7",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-typescript2": "^0.32.1",
		"ts-jest": "^28.0.5",
		"typescript": "^4.7.4"
	},
	"files": [
		"dist"
	],
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node"
	}
}
